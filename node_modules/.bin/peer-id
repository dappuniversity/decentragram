#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//usr/local/bin/node" ]; then
  exec "$basedir//usr/local/bin/node"  "$basedir/../peer-id/src/bin.js" "$@"
else 
  exec /usr/local/bin/node  "$basedir/../peer-id/src/bin.js" "$@"
fi
